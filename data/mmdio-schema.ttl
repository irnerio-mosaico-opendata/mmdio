                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty cito:cites ;
                                                    owl:someValuesFrom :ManuscriptCollection
                                                  ] ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty cito:cites ;
                                                    owl:someValuesFrom memo:Manuscript
                                                  ] ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasSection ;
                                                    owl:someValuesFrom <http://purl.org/spar/doco/Section>
                                                  ] ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty dcterms:creator ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass foaf:Person
                                                  ] ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty frbr:partOf ;
                                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
