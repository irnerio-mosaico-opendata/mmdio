    <!-- https://w3id.org/arco/ontology/core/isLocationOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/core/isLocationOf">
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
