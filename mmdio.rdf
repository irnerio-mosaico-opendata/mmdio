        <owl:inverseOf rdf:resource="http://purl.org/vocab/frbr/core#partOf"/>
        <rdfs:comment xml:lang="en">Meaning = has part.  As defined by FRBR (http://www.ifla.org/VII/s13/frbr/frbr1.htm; http://purl.org/vocab/frbr/core#part),  a property indicating that one entity includes another entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/frbr/core#"/>
        <rdfs:label xml:lang="en">has part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/vocab/frbr/core#partOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/vocab/frbr/core#partOf">
        <rdfs:comment xml:lang="en">Meaning = is part of.  As defined by FRBR (http://www.ifla.org/VII/s13/frbr/frbr1.htm; http://purl.org/vocab/frbr/core#partOf), a property indicating that one entity is a part of another entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/frbr/core#"/>
        <rdfs:label xml:lang="en">is part of</rdfs:label>
    </owl:ObjectProperty>
    


